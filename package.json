{
  "name": "vk-tinker",
  "version": "1.0.0",
  "description": "МАМИН'S КЛАД VK [https://vk.com/maminsklad2015]",
  "scripts": {
    "clean": "rimraf dist",
    "start:server": "nodemon --watch ./src/server ./src/server/index.babel.js --vkTinker:environment development",
    "start:client": "webpack-dev-server --config ./webpack/webpack.development.config --mode development --hot --inline --color",
    "build:server": "babel --copy-files src/server -d dist/server && babel --copy-files src/shared -d dist/shared",
    "build:client": "webpack --config ./webpack/webpack.production.config --mode production --progress --color",
    "build": "npm run build:client && npm run build:server",
    "start:server-prod": "cross-env NODE_ENV=production nodemon --watch ./dist/server ./dist/server/index.js --vkTinker:Environment production",
    "storybook": "start-storybook --ci -p 9002 -c .storybook",
    "tsc": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SchmerZ/vk-tinker.git"
  },
  "keywords": [
    "vk",
    "МАМИН'S КЛАД [https://vk.com/maminsklad2015]"
  ],
  "author": "Vadim Sheydakov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SchmerZ/vk-tinker/issues"
  },
  "homepage": "https://github.com/SchmerZ/vk-tinker#readme",
  "devDependencies": {
    "@babel/cli": "7.8.0",
    "@babel/core": "7.8.0",
    "@babel/plugin-proposal-class-properties": "7.8.0",
    "@babel/plugin-transform-spread": "7.8.0",
    "@babel/preset-env": "7.8.2",
    "@babel/preset-react": "7.8.0",
    "@babel/preset-typescript": "7.8.0",
    "@babel/register": "7.8.0",
    "@storybook/addon-actions": "5.3.1",
    "@storybook/addon-docs": "5.3.1",
    "@storybook/addons": "5.3.1",
    "@storybook/react": "5.3.1",
    "@types/moment": "2.13.0",
    "@types/node": "13.1.6",
    "@types/react": "16.9.17",
    "@types/react-dom": "16.9.4",
    "babel-loader": "8.0.6",
    "css-loader": "3.4.2",
    "node-sass": "^4.13.0",
    "nodemon": "2.0.2",
    "pino-pretty": "3.5.0",
    "react-is": "16.12.0",
    "sass-loader": "8.0.1",
    "style-loader": "1.1.2",
    "typescript": "3.7.4",
    "typings-for-css-modules-loader": "1.7.0"
  },
  "dependencies": {
    "fastify": "2.11.0",
    "fastify-cookie": "3.4.1",
    "fastify-session": "3.2.0",
    "express-session": "1.17.0",
    "memorystore": "1.6.1",
    "nconf": "0.10.0",
    "isomorphic-fetch": "2.2.1",
    "pino": "5.16.0",
    "react": "16.12.0",
    "react-dom": "16.12.0"
  }
}
