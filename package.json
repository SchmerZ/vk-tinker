{
  "name": "vk-tinker",
  "version": "1.0.0",
  "description": "МАМИН'S КЛАД VK [https://vk.com/maminsklad2015]",
  "scripts": {
    "clean": "rimraf dist",
    "start:server": "nodemon --watch ./src/server ./src/server/index.babel.js --vkTinker:environment development",
    "start:client": "webpack-dev-server --config ./webpack/webpack.development.config --mode development --hot --inline --color",
    "build:server": "babel --copy-files src/server -d dist/server && babel --copy-files src/shared -d dist/shared",
    "build:client": "webpack --config ./webpack/webpack.production.config --mode production --progress --color",
    "build": "npm run build:client && npm run build:server",
    "start:server-prod": "cross-env NODE_ENV=production nodemon --watch ./dist/server ./dist/server/index.js --vkTinker:Environment production",
    "tunnel": "lt -h  http://localtunnel.me --subdomain vktinker --port 8090",
    "storybook": "start-storybook --ci -p 9002 -c .storybook",
    "tsc": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SchmerZ/vk-tinker.git"
  },
  "keywords": [
    "vk",
    "МАМИН'S КЛАД [https://vk.com/maminsklad2015]"
  ],
  "author": "Vadim Sheydakov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SchmerZ/vk-tinker/issues"
  },
  "homepage": "https://github.com/SchmerZ/vk-tinker#readme",
  "devDependencies": {
    "webpack": "4.42.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3",
    "html-webpack-plugin": "4.0.0-beta.11",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "react-dev-utils": "10.2.0",
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.7",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-transform-spread": "7.8.3",
    "@babel/preset-env": "7.8.7",
    "@babel/preset-react": "7.8.3",
    "@babel/preset-typescript": "7.8.3",
    "@babel/register": "7.8.6",
    "@storybook/addon-actions": "5.3.17",
    "@storybook/addon-docs": "5.3.17",
    "@storybook/addons": "5.3.17",
    "@storybook/react": "5.3.17",
    "@types/moment": "2.13.0",
    "@types/node": "13.9.1",
    "@types/react": "16.9.23",
    "@types/react-dom": "16.9.5",
    "babel-loader": "8.0.6",
    "css-loader": "3.4.2",
    "node-sass": "4.13.1",
    "nodemon": "2.0.2",
    "pino-pretty": "3.6.1",
    "react-is": "16.13.0",
    "sass-loader": "8.0.2",
    "style-loader": "1.1.3",
    "typescript": "3.8.3",
    "localtunnel": "2.0.0"
  },
  "dependencies": {
    "fastify": "2.12.1",
    "fastify-cookie": "3.6.0",
    "fastify-session": "3.3.0",
    "express-session": "1.17.0",
    "memorystore": "1.6.2",
    "nconf": "0.10.0",
    "isomorphic-fetch": "2.2.1",
    "pino": "5.17.0",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "classnames": "2.2.6"
  }
}
